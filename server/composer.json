{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "https://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "https://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=8.2",
    "ext-fileinfo": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "guzzlehttp/guzzle": "^7.8",
    "illuminate/database": "^11.3",
    "illuminate/events": "^11.3",
    "illuminate/pagination": "^11.3",
    "illuminate/redis": "^11.3",
    "monolog/monolog": "^2.0",
    "php-parallel-lint/php-console-color": "^1.0",
    "robmorgan/phinx": "^0.16.0",
    "sebastian/diff": "^6.0",
    "symfony/cache": "^7.0",
    "symfony/var-dumper": "^7.0",
    "topthink/think-validate": "^2.0",
    "vlucas/phpdotenv": "^5.6",
    "w7corp/easywechat": "^6.15",
    "webman/admin": "dev-main",
    "webman/captcha": "^1.0",
    "webman/console": "^1.3",
    "webman/event": "^1.0",
    "webman/redis-queue": "^1.3",
    "workerman/crontab": "^1.0",
    "workerman/webman-framework": "dev-master"
  },
  "repositories": [
    {
      "url": "git@github.com:webman-php/admin.git",
      "type": "vcs"
    },
    {
      "url": "git@github.com:walkor/webman-framework.git",
      "type": "vcs"
    }
  ],
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "app",
      "App\\": "app",
      "app\\View\\Components\\": "app/view/components"
    },
    "files": [
      "support/helpers.php"
    ]
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  }
}
